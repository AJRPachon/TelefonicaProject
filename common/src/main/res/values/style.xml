<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="user_name_text_style_bold" parent="android:Widget.TextView">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">@dimen/dimen18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">@dimen/dimen0dp</item>
        <item name="android:layout_marginStart">@dimen/dimen10dp</item>
        <item name="android:layout_marginEnd">@dimen/dimen10dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="user_name_text_style_normal" parent="user_name_text_style_bold">
        <item name="android:textStyle">normal</item>
    </style>
</resources>